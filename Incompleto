#include <stdio.h>
#include <stdlib.h>

int main()
{
int tipodeperfil;
double espessuramesa1;
double larguradamesa1;
double tamanhodaalma;
double espessuradaalma;
double centroidex;
double centroidey;
double areadamesa1;
double areamesa2;
double areadaalma;
double dy;
double dx;
printf("Digite o tipo de perfil\n");
printf("1 - > Para perfil H\n");
printf("2 - > Para perfil T\n");
printf("3 - > Para perfil quantoneira\n");
printf("4 - > Para perfil quantoneira dupla\n");
    scanf("%d",&tipodeperfil);
        switch (tipodeperfil) { // SWICH chave que abre o tipo de perfil metalico
            case 1: printf("Digite a largura da mesa inferior(CM)\n");
                        scanf("%lf", &larguradamesa1);
                    printf("Digite a espessura da mesa inferior (CM)\n");
                        scanf("%lf",&espessuramesa1);
                    printf("Digite o tamanho da alma(CM)\n");
                        scanf("%lf", &tamanhodaalma);
                    printf("Digite a espessura da alma\n");
                        scanf("%lf", &espessuradaalma);
                    centroidex = larguradamesa1/2;
                    centroidey = (tamanhodaalma/2)+espessuramesa1;
                        printf("Eixo X = %.2lf Eixo Y = %.2lf\n", centroidex, centroidey);
                    dy = (tamanhodaalma/2)+(espessuramesa1/2);
                    printf("dy = %lf\n",dy);
                    double ix0;
                    double ixfig1;
                    double dy2;
                    double dya;
                    double areafig1;
                    ix0 = espessuramesa1*espessuramesa1*espessuramesa1*larguradamesa1/12;
                    printf("ix0 = %lf\n", ix0);
                    areafig1 = espessuramesa1*larguradamesa1;
                    printf("area da figura = %lf\n", areafig1);
                    dya = dy*dy*areafig1;
                    printf("dya = %lf\n", dya);
                    ixfig1 = ix0 + dya;
                    printf("%lf\n", ixfig1);
    } // SWICH chave que fecha o tipo de perfil metalico

   return 0;
}
